{
  "name": "andy87/yii2-file-crafter",
  "description": "PHP library for Yii2 - developed by and_y87",
  "keywords": ["and_y87", "php", "generator", "crafter", "yii2", "library"],
  "homepage": "https://andy87.ru",
  "license": "CC-BY-SA-4.0",

  "version": "1.0.1",
  "minimum-stability": "dev",

  "prefer-stable": true,

  "authors": [
    {
      "name": "Kidin Andrey",
      "email": "kidin_a@list.ru"
    }
  ],
  "require": {
    "php": ">=8.0",
    "yiisoft/yii2-gii": "~2.2.0",
    "yidas/yii2-composer-bower-skip": "~2.0"
  },

  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },

  "autoload": {
    "psr-4": {
      "andy87\\yii2\\file_crafter\\": "src/"

    }
  },

  "autoload-dev": {
    "psr-4": {
      "andy87\\knock_knock\\": "src/",
      "andy87\\yii2\\file_crafter\\test": "test/"
    }
  },

  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
